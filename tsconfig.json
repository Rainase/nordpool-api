{
  "compilerOptions": {
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "strictNullChecks": true,
      "target": "ES2022",
      "moduleResolution": "Node10",
      "module": "CommonJS",
      "declaration": true,
      "isolatedModules": true,
      "noEmit": true,
      "outDir": "lib"
  },
  "include": ["src", "testing/test.ts"],
  "exclude": ["node_modules"]
}